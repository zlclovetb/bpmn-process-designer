{
  "name": "bpmn-process-designer",
  "version": "0.0.1",
  "description": "A process designer base on Vue 2.x and ElementUI",
  "keywords": [
    "bpmn",
    "bpmn.js",
    "vue"
  ],
  "author": {
    "name": "MiyueFE",
    "email": "913784771@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:miyuesc/bpmn-process-designer.git"
  },
  "scripts": {
    "demo": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "demo mock": "vue-cli-service serve --mode mock"
  },
  "dependencies": {
    "bpmn-js-task-resize": "^1.2.0",
    "bpmn-js-token-simulation": "^0.10.0",
    "bpmnlint-plugin-local": "file:plugin",
    "core-js": "^3.8.1",
    "highlight.js": "^10.5.0",
    "min-dash": "^3.5.2",
    "mockjs": "^1.1.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.26.8",
    "vue-router": "^3.4.9",
    "x2js": "^3.4.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "bpmn-js": "^8.8.3",
    "bpmn-js-bpmnlint": "^0.18.0",
    "bpmn-js-properties-panel": "^0.37.2",
    "bpmnlint": "^7.2.1",
    "bpmnlint-loader": "^0.1.4",
    "camunda-bpmn-moddle": "^4.4.1",
    "compression-webpack-plugin": "^6.1.1",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^8.1.0",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "terser-webpack-plugin": "^4.2.3",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-merge": "^5.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-use-before-define": "off",
      "no-unused-vars": "off",
      "prettier/prettier": [
        "warn",
        {
          "useTabs": false,
          "singleQuote": false,
          "printWidth": 160,
          "semi": true,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "arrowParens": "avoid",
          "requirePragma": false,
          "proseWrap": "preserve"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "Apache"
}
